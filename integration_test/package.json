{
  "name": "tcc-ufu-integration-tests",
  "version": "1.0.0",
  "description": "Integration tests for TCC-UFU Customer API using Playwright",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
